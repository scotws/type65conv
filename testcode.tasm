        .origin 00:8000
        .mpu 65816

start           
                ldx.# 00
                txa
                adc.l 10:0000
loop            sta.x 1000  ; label on line with opcode
                dex
                bne loop

                stp
data  ; with comment after label
        .byte 00, 01, 02, 03, "that's all"
        .end
